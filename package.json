{
  "name": "graasp-plugin-validation",
  "author": "Graasp Association",
  "version": "0.1.0",
  "description": "Repository for validation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "hooks:uninstall": "husky uninstall",
    "hooks:install": "husky install",
    "pre-commit": "pretty-quick --staged && yarn lint",
    "prepare": "yarn build",
    "prepack": "yarn build",
    "prettier:check": "prettier --check \"{src,test}/**/*.ts\"",
    "prettier:write": "prettier --write \"{src,test}/**/*.ts\"",
    "test": "jest  --forceExit --coverage --verbose",
    "test:watch": "yarn test --watchAll",
    "lint": "eslint ."
  },
  "repository": "git@github.com:graasp/graasp-plugin-validation.git",
  "license": "AGPL",
  "bugs": {
    "url": "https://github.com/graasp/graasp-plugin-validation/issues"
  },
  "homepage": "https://github.com/graasp/graasp-plugin-validation#readme",
  "dependencies": {
    "axios": "0.26.1",
    "bad-words": "3.0.4",
    "french-badwords-list": "1.0.5",
    "graasp-plugin-file": "github:graasp/graasp-plugin-file",
    "graasp-plugin-file-item": "github:graasp/graasp-plugin-file-item",
    "http-status-codes": "2.2.0",
    "js-base64": "3.7.2",
    "mime-types": "2.1.35",
    "striptags": "3.2.0"
  },
  "devDependencies": {
    "@graasp/sdk": "github:graasp/graasp-sdk",
    "@trivago/prettier-plugin-sort-imports": "3.2.0",
    "@types/bad-words": "3.0.1",
    "@types/eslint": "^7.2.5",
    "@types/jest": "27.4.0",
    "@types/mime-types": "2.1.1",
    "@types/node": "^14.14.7",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "bad-words": "3.0.4",
    "eslint": "^7.13.0",
    "fastify": "3.29.1",
    "graasp-test": "github:graasp/graasp-test",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "slonik": "27.1.0",
    "ts-jest": "27.1.2",
    "typescript": "^4.0.5",
    "uuid": "8.3.2"
  },
  "packageManager": "yarn@3.2.1"
}
